<body onload="init()">
    <div class="modal fade" id="basicInfoPersonModal" tabindex="-1" role="dialog" aria-labelledby="basicInfoPersonLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="basicInfoPersonTitle">Basic Information</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group row">
                            <div class="col-sm-5 col-form-label">Gender</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoGender"></b>
                            </div>
                            <div class="col-sm-5 col-form-label">Date of birth</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoDateofBirth"></b>
                            </div>
                            <div class="col-sm-5 col-form-label">Nationality</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoNationality"></b>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="basicInfoFilmModal" tabindex="-1" role="dialog" aria-labelledby="basicInfoFilmLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="basicInfoFilmTitle">Basic Information</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group row">
                            <div class="col-sm-5 col-form-label">Genre(s)</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoGenres"></b>
                            </div>
                            <div class="col-sm-5 col-form-label">Min. Age of Audience</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoMaa"></b>
                            </div>
                            <div class="col-sm-5 col-form-label">Country of Production</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoCop"></b>
                            </div>
                            <div class="col-sm-5 col-form-label">Film Average Rating</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoFar"></b>
                            </div>
                            <div class="col-sm-5 col-form-label">Number of Raters</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoNor"></b>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div>
        <h3 class="my-3" id="pageTitle"></h3>
        <span>
            <a class="btn btn-outline-info btn-sm" href="/Home/Films">Films management</a>
            <a class="btn btn-outline-primary btn-sm" href="/Home/FilmRelatedPersons">Film-related persons management</a>
            <a class="btn btn-outline-info btn-sm" id="seeAllFilmRelations" href="/Home/FilmCrews">See all film-person relations</a>
        </span>
    </div>
    <button type="button" id="aubutton" class="btn btn-secondary btn-lg btn-block my-2" data-toggle="modal" data-target="#addFilmPersonRelationModal">Add/Update film-person relation</button>
    <table id="filmPersonRelTable" class="table table-bordered table-hover table-striped" cellspacing="0">
        <thead>
            <tr>
                <th scope="col">Film-related person (Birthdate) </th>
                <th scope="col">Film (Release Year)</th>
                <th scole="col">Role(s)</th>
                <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
    <div class="modal fade" id="addFilmPersonRelationModal" role="dialog" aria-labelledby="addFilmPersonRelationModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Add/Update film-person relation</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form id="createUserForm" method="post">
                        <div class="form-group row">
                            <label for="personSelect" class="col-sm-2 col-form-label my-2">Person</label>
                            <div class="col-sm-10 my-2">
                                <select class="my-1" id="personSelect"></select>
                            </div>
                            <label for="filmSelect" class="col-sm-2 col-form-label my-2">Film</label>
                            <div class="col-sm-10 my-2">
                                <select class="my-1" id="filmSelect"></select>
                            </div>
                            <label for="rolesSelect" class="col-sm-2 col-form-label my-2">Role(s)</label>
                            <div class="col-sm-10 my-2">
                                <select class="my-1" multiple id="rolesSelect">
                                </select>
                            </div>
                        </div>
                    </form>
                    <div class="alert alert-info" role="alert">
                        <p class="text-justify my-0">
                            <b>
                                <u class="py-2">Note</u><br />
                                If a relation between selected film and person already exists,
                                this operation would instead update the previous relation instead of creating a new one!
                            </b>
                        </p>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary" id="btnAddFilmPersonRelation" data-dismiss="modal">Submit</button>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
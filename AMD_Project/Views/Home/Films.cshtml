<body onload="init()">
    <div>
        <h3 id="pageTitle" class="my-3"></h3>
        <a class="btn btn-outline-primary btn-sm my-2" id="backToRootFilms" href="/Home/Films">See all films</a>
    </div>
    <button type="button" id="aubutton" class="btn btn-secondary btn-lg btn-block my-2" data-toggle="modal" data-target="#createFilmModal">Add new film</button>
    <table id="filmTable" class="table table-bordered table-hover table-striped" cellspacing="0">
        <thead>
            <tr>
                <th scope="col">Title</th>
                <th scope="col">Release Year</th>
                <th scope="col">Genre</th>
                <th scope="col">Min. Age of Audience</th>
                <th scope="col">Country of Production</th>
                <th scope="col">Film Average Rating</th>
                <th scope="col">Number of Raters</th>
                <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
    <b id="noFilmSuggestions" class="my-2"></b>
    <div class="modal fade" id="createFilmModal" role="dialog" aria-labelledby="createFilmModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Add a film</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form id="createFilmForm" method="post">
                        <div class="form-group row">
                            <label for="title" class="col-sm-4 col-form-label my-2">Title</label>
                            <div class="col-sm-8 my-2">
                                <input type="text" class="form-control" id="title">
                            </div>
                            <label for="releaseYear" class="col-sm-4 col-form-label my-2">Release Year</label>
                            <div class="col-sm-8 my-2">
                                <input type="number" min="1850" placeholder="Input must be an integer within 1888 to 2022" class="form-control" id="releaseYear">
                            </div>
                            <label for="genre" class="col-sm-4 col-form-label my-2">Genre(s)</label>
                            <div class="col-sm-8 my-2">
                                <select id="genre" class="my-2" multiple></select>
                            </div>
                            <label for="minAgeOfAudience" class="col-sm-4 col-form-label my-2">Min. Age of Audience</label>
                            <div class="col-sm-8 my-2">
                                <input type="number" min="1" max="21" placeholder="Input must be an integer within 1 to 21 or leave this field empty in case there is no age restriction" class="form-control" id="minAgeOfAudience">
                            </div>
                            <label for="countryOfProduction" class="col-sm-4 col-form-label my-2">Country of Production</label>
                            <div class="col-sm-8 my-2">
                                <select id="countryOfProduction" class="my-2"></select>
                            </div>
                            <label for="subordinatedTo" class="col-sm-4 col-form-label my-2" id="labelSubordinatedTo">Subordinated To</label>
                            <div class="col-sm-8 my-2" id="divSubordinatedTo">
                                <select id="subordinatedTo" class="my-2"></select>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary" id="btnCreateFilm" data-dismiss="modal">Submit</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="basicInfoFilmModal" tabindex="-1" role="dialog" aria-labelledby="basicInfoFilmLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="basicInfoFilmTitle">Basic Information</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group row">
                            <div class="col-sm-5 col-form-label">Genre(s)</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoGenres"></b>
                            </div>
                            <div class="col-sm-5 col-form-label">Min. Age of Audience</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoMaa"></b>
                            </div>
                            <div class="col-sm-5 col-form-label">Country of Production</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoCop"></b>
                            </div>
                            <div class="col-sm-5 col-form-label">Film Average Rating</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoFar"></b>
                            </div>
                            <div class="col-sm-5 col-form-label">Number of Raters</div>
                            <div class="col-sm-7 my-2">
                                <b id="basicInfoNor"></b>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
